{
    "version": 2,
    "builds": [
        {
            "src": "dist/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/server.js"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "PORT": "3000",
        "MONGO_URI": "@MONGO_URI"
    }
}